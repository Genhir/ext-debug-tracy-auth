		"auth", 
		"authentication", 
		"authorization", 
		"authenticate", 
		"user", 
		"signin", 
		"signout", 
		"login", 
		"logout", 
		"sign in", 
		"sign out", 
		"log in", 
		"log out"
	],
	"license": ["BSD-3-Clause"],
	"authors": [
		{
			"name": "Tom Flidr",
			"homepage": "https://github.com/tomFlidr"
		}
	],
	"require": {
		"php": ">=5.3.0",
		"mvccore/mvccore": "^4.2",
		"mvccore/ext-auth": "^4.2",
		"mvccore/ext-debug-tracy": "^4.2"
	},
	"require-dev": {
		"php": ">=5.3.0",
		"mvccore/mvccore": "~4.2@dev",
		"mvccore/ext-auth": "~4.2@dev",
		"mvccore/ext-debug-tracy": "~4.2@dev"
	},
	"autoload": {
		"classmap": ["src/"]
	},
	"minimum-stability": "dev"
}
